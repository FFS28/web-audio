{
    "apps": [
        {
            "assets": [
                "assets"
            ],
            "environmentSource": "environments/environment.ts",
            "environments": {
                "dev": "environments/environment.ts",
                "prod": "environments/environment.prod.ts"
            },
            "index": "index.html",
            "main": "main.ts",
            "outDir": "build",
            "polyfills": "polyfills.ts",
            "prefix": "wac",
            "root": "src",
            "scripts": [
                "../node_modules/hammerjs/hammer.js",
                "../node_modules/prismjs/prism.js"
            ],
            "styles": [
                "../node_modules/prismjs/themes/prism-okaidia.css",
                "styles/styles.css"
            ],
            "test": "../config/karma/test.ts",
            "testTsconfig": "../test/unit/tsconfig.json",
            "tsconfig": "tsconfig.json"
        }
    ],
    "defaults": {
        "class": {
            "spec": true
        },
        "component": {
            "changeDetection": "OnPush",
            "inlineStyle": false,
            "inlineTemplate": false,
            "spec": true
        },
        "directive": {
            "spec": true
        },
        "module": {
            "spec": true
        },
        "interface": {
            "prefix": "I"
        },
        "pipe": {
            "spec": true
        },
        "service": {
            "spec": true
        },
        "styleExt": "css"
    },
    "e2e": {
        "protractor": {
            "config": "./config/protractor/e2e.js"
        }
    },
    "project": {
        "name": "web-audio-conference-2016"
    },
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "test": {
        "karma": {
            "config": "./config/karma/config.js"
        }
    }
}
