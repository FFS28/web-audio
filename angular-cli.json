{
    "apps": [
        {
            "assets": [
                "assets"
            ],
            "environments": {
                "dev": "environments/environment.ts",
                "prod": "environments/environment.prod.ts",
                "source": "environments/environment.ts"
            },
            "index": "index.html",
            "main": "main.ts",
            "mobile": false,
            "outDir": "build",
            "polyfills": "polyfills.ts",
            "prefix": "wac",
            "root": "src",
            "scripts": [
                "../node_modules/hammerjs/hammer.js",
                "../node_modules/prismjs/prism.js"
            ],
            "styles": [
                "../node_modules/prismjs/themes/prism-okaidia.css",
                "styles/styles.css"
            ],
            "test": "../config/karma/test.ts",
            "tsconfig": "tsconfig.json"
        }
    ],
    "defaults": {
        "changeDetection": "OnPush",
        "inline": {
            "style": false,
            "template": false
        },
        "prefixInterfaces": false,
        "spec": {
            "class": false,
            "component": true,
            "directive": true,
            "module": false,
            "pipe": true,
            "service": true
        },
        "styleExt": "css"
    },
    "e2e": {
        "protractor": {
            "config": "./config/protractor/e2e.js"
        }
    },
    "project": {
        "name": "web-audio-conference-2016",
        "version": "1.0.0-beta.30"
    },
    "test": {
        "karma": {
            "config": "./config/karma/config.js"
        }
    }
}
