{
    "extends": "tslint-config-holy-grail",
    "rules": {
        "component-selector": [ true, "element", "wac", "kebab-case" ],
        "directive-selector": [ true, "attribute", "wac", "camelCase" ],
        "import-blacklist": false,
        "no-implicit-dependencies": [ true, "dev" ],
        "pipe-naming": [ true, "camelCase", "wac" ]
    }
}
