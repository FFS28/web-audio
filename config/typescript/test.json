{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": false,
    "rootDir": "./",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5"
  },
  "exclude": [
    ".git",
    ".tscache",
    "build",
    "config",
    "node_modules"
  ]
}
